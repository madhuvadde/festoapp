<h2>Add Fest</h2>
<%= form_for @fest do |f| %>
	<label>Name</label>
	<%= f.text_field :name %><br />
	<label>Description :</label>
	<%= f.text_area :description,rows:10,cols:25 %><br />
	<label>start Date</label>
	<%= f.date_field :start_date %><br />
	<label>End Date</label>
	<%= f.date_field :end_date %><br />
	<%= f.submit %>
	<% end%>


